<!DOCTYPE html>
<html>
<head>
  <title>Octothorpe</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id="header-container">
    <div id="header">
      <div id="company-name">
        Octothor.pe
      </div>
      <div id="top-nav-container">
        <%= render 'shared/top_menu' %>
      </div>
    </div>
  </div>
  <div id="page-content">
    <%= yield %>
  </div>
  <div id="footer">
    <%= render 'shared/copyright' %>
  </div>
</body>
</html>
